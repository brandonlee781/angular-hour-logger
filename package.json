{
  "name": "angular-hour-logger",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --watchAll",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint && stylelint 'src/**/*.scss'",
    "format": "prettier src/**/*.{js,jsx,ts,tsx,css,scss,md} --write",
    "e2e": "ng e2e",
    "precommit": "lint-staged",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@ecodev/fab-speed-dial": "^0.0.4",
    "@mat-datetimepicker/core": "^1.0.2",
    "@swimlane/ngx-charts": "^7.2.0",
    "angular-calendar": "^0.23.7",
    "apollo-angular": "^1.0.1",
    "apollo-angular-link-http": "^1.0.2-beta.0",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link-context": "^1.0.7",
    "apollo-link-error": "^1.0.7",
    "core-js": "^2.4.1",
    "d3": "^5.0.0",
    "date-fns": "^1.29.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "html2canvas": "^1.0.0-alpha.12",
    "jest": "^22.4.2",
    "jest-preset-angular": "^5.2.1",
    "jspdf": "^1.3.5",
    "rxjs": "^5.5.7",
    "stylelint": "^9.2.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/html2canvas": "^0.0.33",
    "@types/jest": "^22.2.0",
    "@types/jspdf": "^1.1.31",
    "@types/node": "~6.0.60",
    "babel-core": "^6.26.0",
    "codelyzer": "^4.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "protractor": "~5.1.2",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "~2.5.3"
  },
  "majestic": {
    "jestConfig": "./jest.config.js"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write",
      "git-add"
    ]
  }
}
